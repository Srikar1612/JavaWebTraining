package com.ibm.staticfeature;

class Test{
	static int num;
	public Test() {
		num=10;
	}
	public Test(int num) {
		this.num=num;
	}
	static {
		num=20;
	}
	@Override
	public String toString() {
		return "num: "+num; 
	}
}

public class Application
{
	public static void main( String[] args ) throws ClassNotFoundException
	{
//		Test test=new Test();
//		System.out.println(test.toString());
		Class<?> tClass= Class.forName("com.ibm.staticfeature.Test");
		System.out.print(tClass);
	}
}