package com.ibm.arrays;

class Point {
	public int x;
	public int y;
	public Point(int x, int y) {
		this.x=x;
		this.y=y;
	}
	
	@Override
	public String toString() {
		return "[x: "+x+", y: "+y+"]";
	}
}

public class Application
{
	public static void main( String[] args )
	{
		int[][] nums;
		nums=new int[3][2];
		nums[1][1]=4;
	}

	
}